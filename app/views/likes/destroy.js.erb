$('.likes-count-<%=@post.id%>').text(<%=@post.likes.count%> + " Likes")
$('#likes-<%=@post.id%>').html('<%= j link_to ('<i class="fa fa-thumbs-up text-capitalize text-muted"> Like</i>').html_safe, post_likes_path(@post), method: :post, remote: true, form: {style: 'display:inline-block;'}, class:"card-link"%>')